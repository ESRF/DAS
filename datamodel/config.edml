targetNamespace "DAS"


complex type Server {
    tangoDevice : string
    tangoHost : string
    tangoServerName : string
    principalServer : ServerData
    alternativeServer : ServerData [] optional
}

complex type ServerData {
    host : string
    startScriptPath : string
    stopScriptPath : string
}

complex type DASConfig {
    EDNA : Server
    Workflow : Server optional
}